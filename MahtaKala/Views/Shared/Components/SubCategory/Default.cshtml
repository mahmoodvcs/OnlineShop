@model List<IdValModel>
@if (Model.Count() != 0)
{
    <div class="breadcrumb-area mt-10">
        <div class="container">
            <div class="breadcrumb">
                @foreach (var item in Model)
                {
                    <a class="btn btn-info" href="@Url.Content($"~/home/category?id={item.Id}")">@item.Title</a>
                    <text>
                        &nbsp;
                    </text>
                }
            </div>
        </div>
    </div>
}